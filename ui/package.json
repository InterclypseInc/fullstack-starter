{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test"
  },
  "dependencies": {
    "@material-ui/core": "4.9.11",
    "@material-ui/icons": "4.9.1",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "core-js": "^3.6.4",
    "flux-standard-action": "2.1.1",
    "formik": "^2.1.7",
    "moment": "^2.26.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "7.1.3",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-router-redux": "4.0.8",
    "react-scripts": "5.0.1",
    "redux": "4.0.5",
    "redux-actions": "2.6.5",
    "redux-logger": "3.0.6",
    "redux-promise-middleware": "6.1.2",
    "reselect": "^4.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "semi": [
        "error",
        "never"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "space-before-function-paren": [
        "error",
        "never"
      ],
      "padded-blocks": [
        "error",
        "never"
      ],
      "quote-props": [
        "error",
        "as-needed"
      ],
      "dot-location": [
        "error",
        "property"
      ],
      "func-style": [
        "error",
        "declaration",
        {
          "allowArrowFunctions": true
        }
      ],
      "no-var": [
        "error"
      ],
      "arrow-body-style": [
        "error",
        "as-needed"
      ],
      "sort-imports": [
        "error",
        {
          "ignoreCase": true,
          "memberSyntaxSortOrder": [
            "all",
            "single",
            "multiple",
            "none"
          ]
        }
      ],
      "one-var": [
        "error",
        "never"
      ],
      "id-length": [
        "error",
        {
          "exceptions": [
            "r",
            "g",
            "b",
            "a"
          ]
        }
      ],
      "no-extra-parens": [
        "error",
        "all",
        {
          "nestedBinaryExpressions": false
        }
      ],
      "max-lines": [
        "error",
        {
          "max": 600
        }
      ],
      "max-len": [
        "warn",
        {
          "code": 120
        }
      ],
      "keyword-spacing": [
        "warn"
      ],
      "key-spacing": [
        "warn"
      ],
      "block-spacing": "warn",
      "max-statements": [
        "error",
        {
          "max": 40
        }
      ],
      "object-curly-spacing": [
        "warn",
        "always"
      ],
      "no-multi-spaces": "warn",
      "no-trailing-spaces": "error",
      "camelcase": "warn",
      "no-console": [
        "warn"
      ],
      "no-warning-comments": [
        "warn"
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  }
}
